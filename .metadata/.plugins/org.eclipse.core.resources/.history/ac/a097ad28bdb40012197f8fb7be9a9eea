package henner.a13;

public class BinomialCoefficient {
	
	private int n;
	private int k;
	
	public BinomialCoefficient(int n, int k) throws IllegalArgumentException{
		if(n == null || k == null || n < k )
			throw new IllegalArgumentException("n oder k sind ungueltig gewaehlt.");
		this.n = n;
		this.k = k;
	}

}
